{
    "name": "addgod/nova-cms",
    "description": "A full CMS for nova, using nova resources.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "infinety-es/nova-filemanager": "^2.5",
        "addgod/component-field": "^2.0",
        "addgod/nova-translate-field": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Addgod\\NovaCms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Addgod\\NovaCms\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
